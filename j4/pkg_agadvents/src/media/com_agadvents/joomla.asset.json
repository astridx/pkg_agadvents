{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "com_agadvents",
  "version": "1.0.0",
  "description": "Joomla CMS",
  "license": "GPL-2.0-or-later",
  "assets": [
    {
      "name": "com_agadvents.leaflet.css",
      "type": "style",
      "uri": "com_agadvents/leaflet.css"
    },
    {
      "name": "com_agadvents.leaflet.draw.css",
      "type": "style",
      "uri": "com_agadvents/leaflet.draw.css"
    },
    {
      "name": "com_agadvents.validatecords",
